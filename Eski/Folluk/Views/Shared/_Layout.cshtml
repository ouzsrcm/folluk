<body>
    <div class="page-header">
        <div class="page-header-top">
            <div class="container">
                <div class="page-logo">
                    <a href="/Farm"><img src="/assets/admin/layout3/img/logo-default.png" alt="Folluk" class="logo-default"></a>
                </div>
                <a href="javascript:;" class="menu-toggler"></a>
                <div class="top-menu">
                    @Html.Partial("Topmenu")
                </div>
            </div>
        </div>
        <div class="page-header-menu">
            <div class="container">
                <form class="search-form" action="/Search" method="GET">
                    <div class="input-group">
                        <input type="text" class="form-control" placeholder="ara" name="query">
                        <span class="input-group-btn">
                            <a href="/Search" class="btn submit"><i class="icon-magnifier"></i></a>
                        </span>
                    </div>
                </form>
                <div class="hor-menu ">
                    <ul class="nav navbar-nav">
                        <li class="menu-dropdown mega-menu-dropdown ">
                            <a href="/Farm">
                                <i class="icon-home"></i>
                                Çiftliğim
                            </a>
                        </li>
                        <li class="menu-dropdown classic-menu-dropdown ">
                            <a href="/Account/Profile">
                                <i class="icon-user"></i>
                                Hesabım
                            </a>
                        </li>
                        <li class="menu-dropdown mega-menu-dropdown mega-menu-full ">
                            <a href="/Account/Others">
                                <i class="icon-users"></i>
                                Diğer Kullanıcılar
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div class="page-container">
        <div class="page-head">
            <div class="container">
                <div class="page-title">
                    <h1>@ViewBag.Title<small> @ViewBag.Description</small></h1>
                </div>
            </div>
        </div>
        <div class="page-content">
            <div class="container">
                <div class="row margin-top-10">
                    <div class="col-md-12">
                        @Html.Partial("_Sidebar")
                        <div class="profile-content">
                            @RenderBody()
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="page-prefooter">
        <div class="container">
            <div class="row">
                <div class="col-md-3 col-sm-6 col-xs-12 footer-block">
                    <h2>KİMDİR NE NEDİR?</h2>
                    <p>
                        online çiftlik. farmville gibi değil, folville gibi :)
                    </p>
                </div>
                <div class="col-md-3 col-sm-6 col-xs12 footer-block">
                    <h2>EPOSTANI BIRAK</h2>
                    <div class="subscribe-form">
                        <form action="javascript:;">
                            <div class="input-group">
                                <input type="text" placeholder="mail@email.com" class="form-control">
                                <span class="input-group-btn">
                                    <button class="btn" type="submit">KAYDET</button>
                                </span>
                            </div>
                        </form>
                    </div>
                </div>
                <div class="col-md-3 col-sm-6 col-xs-12 footer-block">
                    <h2>TAKİP ET</h2>
                    <ul class="social-icons">
                        <li>
                            <a href="javascript:;" data-original-title="rss" class="rss"></a>
                        </li>
                        <li>
                            <a href="javascript:;" data-original-title="facebook" class="facebook"></a>
                        </li>
                        <li>
                            <a href="javascript:;" data-original-title="twitter" class="twitter"></a>
                        </li>
                        <li>
                            <a href="javascript:;" data-original-title="googleplus" class="googleplus"></a>
                        </li>
                        <li>
                            <a href="javascript:;" data-original-title="linkedin" class="linkedin"></a>
                        </li>
                        <li>
                            <a href="javascript:;" data-original-title="youtube" class="youtube"></a>
                        </li>
                        <li>
                            <a href="javascript:;" data-original-title="vimeo" class="vimeo"></a>
                        </li>
                    </ul>
                </div>
                <div class="col-md-3 col-sm-6 col-xs-12 footer-block">
                    <h2>Contacts</h2>
                    <address class="margin-bottom-40">
                        Phone: 800 123 3456<br>
                        Email: <a href="mailto:info@metronic.com">info@metronic.com</a>
                    </address>
                </div>
            </div>
        </div>
    </div>
    <div class="page-footer">
        <div class="container">
            @DateTime.Now.Date.Year &copy; <a href="http://www.facebook.com/Srcm.Ouz">Oğuzhan SARIÇAM</a>.
        </div>
    </div>
    <div class="scroll-to-top">
        <i class="icon-arrow-up"></i>
    </div>
